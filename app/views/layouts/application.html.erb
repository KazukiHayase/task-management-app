<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "layouts/header" %>
  
    <div id="flash_message">
      <%= render 'shared/flash' %>
    </div>
    
    <div class="main">
      <%= yield %>
    </div>
  </body>
</html>
