<h1>ログイン画面</h1>
<%= form_with url: login_path, scope: :session, local: true do |f| %>
    <div>
        <%= f.label :email %>
        <br>
        <%= f.email_field :email %>
    </div>
    <div>
        <%= f.label :password %>
        <br>
        <%= f.password_field :password %>
    </div>
    <div>
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
    </div>
    <%= f.submit "ログイン" %>
<% end %>

